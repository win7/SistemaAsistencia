<div class="container">
	<div class="row">
		<div class="col-md-12">
			<h4 class="text-center"> <%= titulo %> </h4>
			<div class="table-responsive">
				<table class="table table-striped">
					<thead>
						<tr>
							<th> Id </th>
							<th> Turno </th>
							<th> Hr. Entrada </th>
							<th> Hr. Salida </th>
							<th> Acci√≥n </th>
						</tr>
					</thead>
					<tbody>
